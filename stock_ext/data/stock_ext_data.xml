<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_piece_seq" model="ir.sequence">
        <field name="name">Stock Piece Sequence</field>
        <field name="code">stock_ext.stock_piece</field>
        <field name="prefix">P</field>
        <field name="padding">5</field>
    </record>

    <record id="tax_mxn" model="account.tax">
        <field name="name">Tax 16%</field>
        <field name="description">Tax 16%</field>
        <field name="sequence">4</field>
        <field name="amount_type">fixed</field>
        <field name="amount" eval="16"/>
    </record>
</odoo>