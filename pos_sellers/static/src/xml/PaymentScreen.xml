<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="PaymentScreenExt" t-inherit="point_of_sale.PaymentScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('payment-controls')]" position="after">
            <t t-foreach="sellers" t-as="seller" t-key="seller.id">
                <div class="button">
                    <span t-esc="seller.name"/>
                </div>
            </t>
        </xpath>
    </t>
</templates>
